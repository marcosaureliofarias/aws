<%
  available_tokens = %w(name firstname lastname)
%>
<% available_tokens.each do |token_name| %>
  <tr>
    <td>
      <%= easy_printable_template_link_to_add_token "%easy_contact_#{token_name}%", f, l("field_easy_contact_#{token_name}") %>
    </td>
    <td>
      <%= l("field_easy_contact_#{token_name}", default: '') %>
    </td>
  </tr>
<% end %>

<% EasyContactCustomField.visible.sorted.each do |cf| %>
  <tr>
    <td><%= easy_printable_template_link_to_add_token("%easy_contact_cf_#{cf.id}%", f, cf.name) %></td>
    <td><%= cf.name %></td>
  </tr>
<% end %>
