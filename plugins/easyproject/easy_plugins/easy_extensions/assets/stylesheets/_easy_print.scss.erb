<% ep_com = Redmine::Plugin.installed?(:easy_project_com)
   er_com = Redmine::Plugin.installed?(:easy_redmine) && EasySetting.value(:ui_theme).presence
 %>
<% if ep_com%>
  @import "easy_project_com/common_assets";
  @import "easy_project_com/components/typography";
  @import "easy_project_com/components/progress";
  @import "easy_project_com/components/avatar";
  @import "easy_project_com/components/buttons";
  @import "easy_project_com/components/modal";
  @import "easy_project_com/components/jqueryui";
<% else %>
  <% if er_com %>
    @import "easy_redmine/common_assets";
  <% else %>
    @import "easy_extensions/common_assets";
  <% end %>
  @import "easy_extensions/components/typography";
  @import "easy_extensions/components/progress";
  @import "easy_extensions/components/avatar";
  @import "easy_extensions/components/buttons";
  @import "easy_extensions/components/modal";
  @import "easy_extensions/components/jqueryui";
<% end %>
$color-print--text: $color-print;
$color-print--accent: lighten($color-main, 50%);
