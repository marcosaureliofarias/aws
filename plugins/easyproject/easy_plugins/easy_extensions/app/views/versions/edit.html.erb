<h2><%=l(:label_version)%></h2>

<%= labelled_form_for @version, :html => {:class => 'form-box', :html => {:multipart => true}} do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } -%>
  <div class="form-actions">
    <%= submit_tag l(:button_save), :class => 'button-positive' -%>
  </div>
<% end %>
