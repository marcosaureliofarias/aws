<h2><%=l(:label_version_new)%></h2>

<%= labelled_form_for @version, :url => project_versions_path(@project), :html => {:html => {:multipart => true}, :class => 'form-box'} do |f| %>
  <%= render :partial => 'versions/form', :locals => { :f => f } %>
  <div class="form-actions">
    <%= submit_tag l(:button_create), :class => 'button-positive' %>
  </div>
<% end %>
