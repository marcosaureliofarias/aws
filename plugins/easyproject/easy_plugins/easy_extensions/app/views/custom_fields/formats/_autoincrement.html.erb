<p>
  <label for="custom_field_settings_from"><%= l(:label_field_format_autoincrement_from_to) %></label>
  <%= number_field_tag 'custom_field[settings][from]', custom_field.settings['from'] || 0, :size => 5 %> -
  <%= number_field_tag 'custom_field[settings][to]', custom_field.settings['to'] || 999, :size => 5 %>
</p>
<% if custom_field.is_a?(IssueCustomField) %>
  <p>
    <%= l(:label_field_format_autoincrement_entity_limitation) %>
  </p>
  <p class="centered-input">
    <%= label_tag 'custom_field_settings_per_project', l(:field_project) %><%= check_box_tag 'custom_field[settings][per_project]', '1', custom_field.settings['per_project'] == '1' %>
  </p>
  <p class="centered-input">
    <%= label_tag 'custom_field_settings_per_tracker', l(:field_tracker) %><%= check_box_tag 'custom_field[settings][per_tracker]', '1', custom_field.settings['per_tracker'] == '1' %>
  </p>
<% end %>
<p>
  <label for="custom_field_min_length"><%=l(:label_min_max_length)%></label>
  <%= f.number_field :min_length, :size => 5, :no_label => true %> -
  <%= f.number_field :max_length, :size => 5, :no_label => true %>
</p>
