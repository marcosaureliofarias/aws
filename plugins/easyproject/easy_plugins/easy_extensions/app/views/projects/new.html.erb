<%= title l(:label_project_new) %>
<%= labelled_form_for @project, :html => {:id => 'project-form', :class => 'form-box', :multipart => true} do |f| %>
  <%= render :partial => 'projects/form', :locals => { :f => f } %>
  <div class="form-actions">
    <%= submit_tag l(:button_create), :class => 'button-positive' %>
    <%= submit_tag l(:button_create_and_continue), :name => 'continue', :class => 'button' %>
  </div>
<% end %>
