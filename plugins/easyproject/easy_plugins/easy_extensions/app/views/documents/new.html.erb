<h2><%=l(:label_document_new)%></h2>

<%= labelled_form_for @document, :url => project_documents_path(@project), :html => {:multipart => true, :class => 'form-box'} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <div class="form-actions"><%= submit_tag l(:button_create), :class => 'button-positive' %></div>
<% end %>
