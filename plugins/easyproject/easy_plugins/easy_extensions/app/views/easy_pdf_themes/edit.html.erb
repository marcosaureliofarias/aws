
<h2><%= link_to l(:label_easy_gantt_theme_plural), easy_pdf_themes_path %> &#187; <%= @pdf_theme %></h2>

<%= labelled_form_for @pdf_theme, :html => {:multipart => true, :class => 'form-box'} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f, :theme => @pdf_theme} %>
  <div class="form-actions"><%= submit_tag l(:button_save), :class => 'button-positive' %></div>
<% end %>
