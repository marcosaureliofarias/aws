<span><%= link_to(report.alert.name, {:controller => 'alerts', :action => 'show', :id => report.alert.id, :only_path => false}, :title => l(:title_alerts_show_alert, :alertname => report.alert.name)) -%></span>
<span><%= link_to(report.entity.name, {:controller => 'versions', :action => 'show', :id => report.entity_id, :only_path => false}, :title => l(:title_alerts_report_entity_project, :entityname => report.entity.name)) -%></span>
<span><%= time_ago_in_words_ex(report.entity.due_date, l(:label_project_due_date_overdue), l(:label_project_due_date_not_overdue)) unless report.entity.due_date.nil? -%></span>
