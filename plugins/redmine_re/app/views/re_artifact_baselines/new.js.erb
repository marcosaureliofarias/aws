$("#ajax-modal").html("<%=j render(partial: 'edit', locals: { baseline: @re_artifact_baseline }) %>");
showModal('ajax-modal', '550px','<%= l(:label_re_artifact_baseline_modal) %>');
$(".ui-dialog-title").html("<%= l(:label_re_artifact_baseline_new) %>");

$('#ajax-modal').dialog( {
  buttons: [
    {
      text: '<%=j l(:button_close) %>',
      click: function() {$(this).dialog('close');}, 'class': 'button'
    },
    {
      text: '<%=j l(:button_save) %>',
      click: function() {
        $('form#baseline-form').submit();

        $(this).dialog('close');
      }, 'class': 'button button-positive'
    }
  ]
});
