var m = $("#ajax-modal");
m.html("<%= j render(template: 'test_case_issue_executions/edit', formats: [:html]) %>");
m.find('.buttons').remove();
showModal(m[0].id, '80%');
var closeButton = {
  text: "<%=j l(:button_close) -%>",
  title: "<%=j l(:button_close) -%>",
  click: function() {hideModal()},
  'class': 'button'
};
var submitButton = {
  text: "<%=j l(:button_update) -%>",
  title: "<%=j l(:button_update) -%>",
  click: function() {$(this).find('form').submit();},
  'class': 'button-positive'
};
var submitAndReportIssueButton = {
  text: "<%=j l(:button_update_and_report) -%>",
  title: "<%=j l(:button_update_and_report) -%>",
  click: function() { $("#report_issue").val("true"); $(this).find('form').submit(); },
  'class': 'button-positive'
};
m.dialog('option', {
  buttons: [closeButton, submitButton, submitAndReportIssueButton]
});